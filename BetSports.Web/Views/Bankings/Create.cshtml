@model BetSports.Web.Models.BankingViewModel

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>Banking</h4>
<hr />
<div class="row">
    <form asp-action="Create" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="Banking.Id" />
        <div class="row">
            <div class="col-md-4">
                <div class="form-group">
                    <label asp-for="Banking.Name" class="control-label"></label>
                    <input asp-for="Banking.Name" class="form-control" />
                    <span asp-validation-for="Banking.Name" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="Banking.Owner" class="control-label"></label>
                    <input asp-for="Banking.Owner" class="form-control" />
                    <span asp-validation-for="Banking.Owner" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="Banking.Location" class="control-label"></label>
                    <input asp-for="Banking.Location" class="form-control" />
                    <span asp-validation-for="Banking.Location" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="BankingSetting.MinutesCancelTicket" class="control-label"></label>
                    <input asp-for="BankingSetting.MinutesCancelTicket" class="form-control" />
                    <span asp-validation-for="BankingSetting.MinutesCancelTicket" class="text-danger"></span>
                </div>
            </div>
        </div>
        <hr />
    </form>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

